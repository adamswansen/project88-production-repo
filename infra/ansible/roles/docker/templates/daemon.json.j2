{
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "100m",
    "max-file": "3"
  },
  "storage-driver": "overlay2",
  "live-restore": true,
  "userland-proxy": false,
  "no-new-privileges": true,
  "default-ulimits": {
    "nofile": {
      "Name": "nofile",
      "Hard": 65536,
      "Soft": 65536
    }
  }{% if docker_insecure_registries is defined %},
  "insecure-registries": {{ docker_insecure_registries | to_json }}{% endif %}{% if docker_registry_mirrors is defined %},
  "registry-mirrors": {{ docker_registry_mirrors | to_json }}{% endif %}
}